{
    "name": "@harukit/cli",
    "version": "0.1.0",
    "description": "CLI tool for Harukit UI components",
    "main": "dist/index.js",
    "bin": {
        "harukit": "dist/index.js"
    },
    "files": [
        "dist",
        "templates"
    ],
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "clean": "rm -rf dist",
        "prepublishOnly": "npm run build",
        "test": "jest",
        "test:watch": "jest --watch",
        "lint": "eslint src --ext .ts",
        "type-check": "tsc --noEmit"
    },
    "keywords": [
        "cli",
        "ui",
        "components",
        "react",
        "typescript",
        "tailwind"
    ],
    "author": "Your Name",
    "license": "MIT",
    "dependencies": {
        "commander": "^11.1.0",
        "prompts": "^2.4.2",
        "zod": "^3.22.4",
        "chalk": "^5.3.0",
        "ora": "^7.0.1",
        "fs-extra": "^11.1.1",
        "glob": "^10.3.10",
        "node-fetch": "^3.3.2",
        "semver": "^7.5.4",
        "cosmiconfig": "^8.0.0",
        "prettier": "^3.1.1",
        "typescript": "^5.3.3"
    },
    "devDependencies": {
        "@types/fs-extra": "^11.0.4",
        "@types/glob": "^8.1.0",
        "@types/node": "^20.10.5",
        "@types/prettier": "^3.0.0",
        "@types/semver": "^7.5.6",
        "tsup": "^8.0.1",
        "jest": "^29.7.0",
        "@types/jest": "^29.5.11",
        "eslint": "^8.56.0",
        "@typescript-eslint/eslint-plugin": "^6.15.0",
        "@typescript-eslint/parser": "^6.15.0"
    },
    "engines": {
        "node": ">=18.0.0"
    }
}